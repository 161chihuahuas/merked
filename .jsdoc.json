{
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "homepageTitle": "merked ~ shred data into a merkle graph"
    }
  },
  "markdown": {
      "hardwrap": false,
      "idInHeadings": true
  }
}
